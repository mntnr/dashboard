{
  "name": "ipfs-project-repos",
  "version": "0.0.0",
  "description": "IPFS repository info",
  "author": "Harlan T Wood <code@harlantwood.net>",
  "license": "MIT",
  "scripts": {
    "build": "browserify src/index.coffee --transform coffeeify --extension='.coffee' -g uglifyify | uglifyjs --mangle --compress > public/bundle.js",
    "dev": "src/server/dev.coffee",
    "publish": "npm run build && bin/publish",
    "watch": "nodemon --watch src --watch public/index.html  --watch public/styles --exec 'browserify' src/index.coffee --transform coffeeify --extension='.coffee' --outfile public/bundle.js"
  },
  "dependencies": {},
  "devDependencies": {
    "bluebird": "^3.0.5",
    "browser-sync": "^2.8.0",
    "browserify": "^12.0.1",
    "coffee-script": "^1.9.3",
    "coffeeify": "^2.0.1",
    "datatables": "^1.10.9",
    "gh-pages": "^0.8.0",
    "jquery": "^2.1.4",
    "lightsaber": "^0.5.1",
    "lodash": "^3.10.0",
    "nodemon": "^1.3.8",
    "octokat": "^0.4.12",
    "request-promise": "^1.0.2",
    "teacup": "^2.0.0",
    "uglify-js": "^2.4.23",
    "uglifyify": "^3.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ipfs/project-repos.git"
  }
}
